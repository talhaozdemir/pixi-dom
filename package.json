  "repository": {
    "type": "git",
    "url": "https://github.com/talhaozdemir/pixi-responsive.git"
  },
  "scripts": {
    "start": "xs serve",
    "watch": "xs watch",
    "build": "xs build",
    "docs": "xs docs",
    "lint": "xs lint",
    "types": "xs types",
    "release": "xs release",
    "test": "xs test"
  },
